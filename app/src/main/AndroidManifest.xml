<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackCompose"
        tools:targetApi="31">
        <activity
            android:name=".RecyclerViewActivity"
            android:exported="false"
            android:label="@string/title_activity_recycler_view"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".SimpleApplicationActivity"
            android:exported="false"
            android:label="@string/title_activity_simple_application"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".ScaffoldActivity"
            android:exported="false"
            android:label="@string/title_activity_scaffold"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".StateActivity"
            android:exported="false"
            android:label="@string/title_activity_state"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".ComposableFunctionActivity"
            android:exported="false"
            android:label="@string/title_activity_composable_function"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".ModifierActivity"
            android:exported="false"
            android:label="@string/title_activity_modifier"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".RowColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_row_column"
            android:theme="@style/Theme.JetpackCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>